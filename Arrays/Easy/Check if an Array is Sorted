//OPTIMAL SOLUTION
Time Complexity: O(N)
Space Complexity: O(1)

class Solution {
    boolean arraySortedOrNot(int[] arr, int n) {
        // code here
        for(int i=1;i<n;i++){
            if(arr[i]>=arr[i-1]){
                
            }
            else return false;
        }
        return true;
    }
}
