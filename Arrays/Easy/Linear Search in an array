//OPTIMAL APPROACH
Time Complexity: O(n), where n is the length of the array.
Space Complexity: O(1)

class Solution{
    static int searchInSorted(int arr[], int N, int K)
    {
        // Your code here
        for(int i=0;i<N;i++){
            if(arr[i]==K){
                return 1;
            }
        }
        return -1;
        
    }
}
