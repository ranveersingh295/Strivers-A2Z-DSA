public class Solution {
    public static double areaSwitchCase(int ch, double []a) {
        // Write your code here
        double area;
        switch(ch){
            case 1:
            double r = a[0];
            area = Math.PI*r*r;
            break;
            case 2:
            double l =a[0];
            double b = a[1];
            area = l *b;
            break;
            default:
            return -1;
  }
    return area;
}
}
